{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenBankingConnector": {
    "BankTests": {
      "TestGroups": [
        {
          "SoftwareStatementProfileId": "All",
          "RegistrationScope": "All",
          "SoftwareStatementAndCertificateProfileOverrides": {
            "NatWest": "NatWest"
          },
          "GenericHostAppTests": [
            "Modelo",
            "NatWest",
            "RoyalBankOfScotland",
            "Danske",
            "Lloyds"
          ],
          "PlainAppTests": [
            "Modelo"
          ]
        },
        {
          "SoftwareStatementProfileId": "PaymentInitiation",
          "RegistrationScope": "PaymentInitiation",
          "GenericHostAppTests": [
            "RoyalBankOfScotland",
            "Monzo"
          ]
        },
        {
          "SoftwareStatementProfileId": "Hsbc_Sandbox",
          "RegistrationScope": "All",
          "GenericHostAppTests": [
            "Hsbc_Sandbox"
          ]
        }
      ],
      "LogExternalApiData": false,
      "ConsentAuthoriser": {
        "NodeJS": {
          "NodeJSProcessOptionsAddInspectBrk": false,
          "OutOfProcessNodeJSServiceOptions": {
            "NumRetries": 0,
            "TimeoutMS": 40000
          }
        },
        "PuppeteerLaunch": {
          "Headless": true,
          "SlowMo": null,
          "DevTools": false,
          "IgnoreExecutablePathAndArgs": true,
          "ExecutablePath": {
            "Windows": "C:/Program Files/Google/Chrome/Application/chrome.exe",
            "MacOs": "~/Library/Application Support/Google/Chrome",
            "Linux": "~/.config/google-chrome/default"
          },
          "Args": []
        }
      }
    }
  }
}
