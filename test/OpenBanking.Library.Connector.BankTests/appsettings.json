{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    },
    "Console": {
      "FormatterName": "Json",
      "FormatterOptions": {
        "IncludeScopes": true,
        "TimestampFormat": "yyyy-MM-ddTHH:mm:ss.fffZ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    }
  },
  "AllowedHosts": "*",
  "OpenBankingConnector": {
    "BankTests": {
      "TestGroups": {
        "All": {
          "SoftwareStatementProfileId": "sandbox",
          "RegistrationScope": "All",
          "GenericHostAppTests": [
            "NatWest_NatWestSandbox",
            "NatWest_RoyalBankOfScotlandSandbox",
            "Danske_Sandbox",
            "Lloyds_Sandbox",
            "Obie_Model2023"
          ],
          "PlainAppTests": [
            "Obie_Model2023"
          ]
        },
        "AccountAndTransaction": {
          "SoftwareStatementProfileId": "sandboxAisp",
          "RegistrationScope": "AccountAndTransaction",
          "GenericHostAppTests": [
            "Monzo_Sandbox"
          ]
        },
        "PaymentInitiation": {
          "SoftwareStatementProfileId": "sandboxPisp",
          "RegistrationScope": "PaymentInitiation",
          "GenericHostAppTests": [
            "Monzo_Sandbox"
          ]
        }
      },
      "LogExternalApiData": false,
      "Auth": {
        "PlaywrightLaunch": {
          "Headless": true,
          "SlowMo": 0,
          "IgnoreExecutablePathAndArgs": true,
          "Args": []
        }
      }
    },
    "ConfigurationSources": {
      "UseUserSecrets": false
    }
  }
}
